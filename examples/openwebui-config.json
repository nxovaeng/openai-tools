{
  "name": "Xray Nginx Manager",
  "description": "Automated Xray + Nginx deployment and configuration management",
  "url": "http://localhost:8000",
  "authentication": {
    "type": "api_key",
    "header": "X-API-Key",
    "value": "your-api-key-here"
  },
  "endpoints": {
    "documentation": "/docs",
    "openapi_schema": "/openapi.json",
