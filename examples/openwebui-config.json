    "Xray VLESS+XHTTP service deployment",
    "Nginx reverse proxy configuration",
    "Multi-domain support",
    "SSL certificate management",
    "Service monitoring",
    "Subscription link generation"
  ],
  "tags": [
    "xray",
    "nginx",
    "proxy",
    "deployment",
    "configuration"
  ]
}
